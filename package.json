{
  "name": "sacco+-admin-portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "build:production": "NODE_ENV=production vite build",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linter configured. Add ESLint or similar.' && exit 0",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "e2e:critical": "playwright test auth.spec.ts allocation.spec.ts",
    "e2e:security": "playwright test security.spec.ts",
    "e2e:rls": "playwright test rls.spec.ts",
    "e2e:smoke": "playwright test smoke.spec.ts",
    "e2e:full": "playwright test",
    "e2e:ci": "CI=true playwright test",
    "test:all": "npm run test && npm run e2e",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "deploy": "npm run build && npx wrangler pages deploy dist --project-name=sacco",
    "generate-icons": "node scripts/generate-icons.mjs",
    "generate-icons:bash": "bash scripts/generate-icons.sh"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@sentry/react": "^10.32.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-virtual": "^3.13.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.26.1",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.3",
    "react-is": "^19.2.3",
    "recharts": "^3.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.21",
    "bundlesize": "^0.18.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "storybook": "^8.6.15",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.53.0",
    "vite": "^6.2.0",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16",
    "web-vitals": "^5.1.0"
  }
}
